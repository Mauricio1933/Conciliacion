#ConciliarButton {
    background: #4C3500;
    border-radius: 10px;
    color: #EFB90B;
    font-weight: bold;
    font-size: 14px;
}

#ConciliarButton:hover {
    background: #5a4000;
}

/* ================================================================================
   ESTADO HABILITADO DEL BOTÓN CONCILIAR (NARANJA)
   ================================================================================ */
#ConciliarButton:enabled {
    background: #EFB90B;
    color: #181A20;
    border: 2px solid #F2CD55;
}

#ConciliarButton:enabled:hover {
    background: #F2CD55;
    color: #181A20;
}

/* ================================================================================
   ESTADO DESHABILITADO DEL BOTÓN CONCILIAR
   ================================================================================ */
#ConciliarButton:disabled {
    background: #2a2a2a;
    color: #666666;
    border: 1px solid #3a3a3a;
}